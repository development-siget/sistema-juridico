@using Juridico.Views.Shared.Components.Sidebar
<!DOCTYPE html>
<html lang="es">
<head>
    @*<meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />*@
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title>@ViewData["Title"] - Sistema de Documentos Jurídico</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">


    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />


    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.css">
    <!-- DataTables -->
    <link rel="stylesheet" href="~/lib/datatables/css/dataTables.bootstrap4.css">
    <link rel="stylesheet" href="~/plugins/datatables-buttons/js/buttons.html5.min.js">
    <link rel="stylesheet" href="~/plugins/jszip/jszip.min.js">
    <link rel="stylesheet" href="~/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js">
    <!-- JasnyBootstrap -->
    <link rel="stylesheet" href="~/lib/jasny-bootstrap/css/jasny-bootstrap.min.css">
   

    <link rel="stylesheet" href="~/lib/select2/css/select2.css" />

    <!-- iCheck for checkboxes and radio inputs -->
    <link rel="stylesheet" href="~/lib/icheck-bootstrap/icheck-bootstrap.min.css">


  
    <!-- Theme style -->
    <link rel="stylesheet" href="~/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">

    <link rel="stylesheet" href="~/css/site.css" />
    <!-- Select2 -->
    <link rel="stylesheet" href="~/lib/select2/css/select2.min.css">
    @*<link rel="stylesheet" href="~/lib/select2-bootstrap4-theme/select2-bootstrap4.min.css">*@
    <!-- dropzonejs -->
    <link rel="stylesheet" href="~/lib/dropzone/min/dropzone.min.css">

    <link rel="stylesheet" href="~/lib/fullcalendar/main.css">

    <!-- Theme style -->
    <link rel="stylesheet" href="~/lib/admin-lte/css/adminlte.min.css">

</head>


<body class="hold-transition sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">


        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-dark">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="#" class="nav-link">Sistema de Documentos Jurídico - SIGET</a>
                </li>
            </ul>

            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">

                <!-- Boton pantalla completa -->
                <li class="nav-item">
                    <a class="nav-link" data-widget="fullscreen" href="#" role="button">
                        <i class="fas fa-expand-arrows-alt"></i>
                    </a>
                </li>

            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Sidebar -->
        @*<partial name="_Sidebar" />*@
        @(await Component.InvokeAsync<SidebarViewComponent>())

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1>@ViewData["Title"]</h1>
                        </div>
                    </div>
                </div><!-- /.container-fluid -->
            </section>

            <!-- Main content -->
            <section class="content">
                <div class="container-fluid">
                    <partial name="_AlertsPartial" />
                    @RenderBody()
                </div>

            </section>
            <!-- /.content -->
        </div>

        <!-- /.content-wrapper -->
        <!-- Popup Modal configurable con JQry -->
        <div class="modal" tabindex="-1" role="dialog" id="form-modal">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title"></h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">×</span>
                        </button>
                    </div>
                    <div class="modal-body">

                    </div>
                </div>
            </div>
        </div>

        <!-- Popup Modal configurable con JQry -->
        <div class="modal" tabindex="-1" role="dialog" id="form-modal-lg">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title"></h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">×</span>
                        </button>
                    </div>
                    <div class="modal-body">

                    </div>
                </div>
            </div>
        </div>

        <div class="loaderbody ocultar" id="loaderbody">
            <div class="loader"></div>
        </div>



        <footer class="main-footer">
            <div class="float-right d-none d-sm-block">
                <b>Version</b> 1.5.0-alfa
            </div>
            <strong>
                Copyright &copy;
                <script>new Date().getFullYear() > document.write(new Date().getFullYear());</script> <a href="http://www.siget.gob.sv">Superintendencia General de Electricidad y Telecomunicaciones</a>.
            </strong>
        </footer>

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->

    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://code.jquery.com/ui/1.13.0/jquery-ui.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/lib/admin-lte/js/adminlte.min.js"></script>
    <!-- InputMask -->
    <script src="~/lib/inputmask/jquery.inputmask.bundle.min.js"></script>
    <!-- dropzonejs -->
    <script src="~/lib/dropzone/min/dropzone.min.js"></script>

    <script src="~/lib/fullcalendar/main.min.js"></script>
    <script src="~/lib/fullcalendar/locales/es.min.js"></script>

    <!-- Filterizr-->
    <script src="~/plugins/filterizr/jquery.filterizr.min.js"></script>
    <!-- Page specific script -->
    <script src="~/lib/select2/js/select2.full.min.js"></script>
    <script src="~/lib/jasny-bootstrap/js/jasny-bootstrap.min.js"></script>
    <script src="~/lib/select2/js/select2.full.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>



    <partial name="_AlertsJSPartial" />
    <!-- Page specific script -->
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
